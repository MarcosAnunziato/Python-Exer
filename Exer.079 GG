# Desafio 079 GG
numeros = []

while True:
    n = int(input('Digite um valor: '))
    if n not in numeros:
        numeros.append(n)
        print('Valor adicionado com sucesso...')
    else:
        print('Valor duplicado! Não vou adicionar...n')

    r = str(input('Quer continuar [S/N] ')).strip().upper()[0]
    if r not in 'SN':
        if r == 'N':
         break

numeros.sort()
print(f'Você digitou os valores {numeros}')