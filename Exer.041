from datetime import date
atual = date.today().year
nascimento = float(input('Ano de nascimento: '))
idade = atual - nascimento
if idade <= 9:
    print('Mirim')
elif idade <= 14:
    print('Infantil')
elif idade <= 19:
    print('Junior')
elif idade <= 25:
    print('Senior')
else:
    idade > 25
    print('Master')
print('Fim')
