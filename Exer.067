# Desafio 67

print('*'*20)
print('      Tabuada')
print('*'*20)



while True:
    n = int(input('Quer ver a tabuada de qual valor? '))
    print('*'*30)
    if n < 0:
        break
    for c in range(1,11):
        print(f'{n} X {c} = {n*c}')
    print('*'*30)

print('Fim do Programa de Tabuada.')

